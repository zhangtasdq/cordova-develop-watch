{
    "devel"           : true,
    "camelcase"       : true,
    "curly"           : true,
    "eqeqeq"          : true,
    "indent"          : 4,
    "noarg"           : true,
    "noempty"         : true,
    "undef"           : true,
    "unused"          : true,
    "strict"          : false,
    "evil"            : false,
    "globalstrict"    : false,
    "loopfunc"        : false,
    "notypeof"        : false,
    "sub"             : false,
    "maxlen"          : 120,
    "shadow"          : false,
    "smarttabs"       : false,
    "proto"           : false,
    "multistr"        : false,
    "laxbreak"        : false,
    "laxcomma"        : false,
    "maxstatements"   : 15,
    "node"            : true,
    "browser"         : true,
    "esversion"       : 6,

    "predef": [
        "$",
        "jQuery",
        "define",
        "describe",
        "expect",
        "it",
        "before",
        "beforeEach",
        "afterEach",
        "after",
        "sinon"
    ]
}
